{
  "buildCommand": "cd src && npm run build",
  "installCommand": "cd src && npm install",
  "outputDirectory": "src/.next",
  "framework": "nextjs",
  "functions": {
    "src/src/app/api/**/*": {
      "maxDuration": 60 
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/src/app/api/$1"
    }
  ]
}